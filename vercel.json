{
    "version": 2,
    "builds": [
      {
        "src": "frontend/build/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/build/$1"
      }
    ]
  }